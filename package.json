{
  "name": "react-component-starter",
  "version": "0.0.0",
  "main": "dist/Component.js",
  "jsnext:main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "demo-bundle": "webpack --config ./webpack-dev.config.js",
    "demo-watch": "webpack --config ./webpack-dev.config.js --watch",
    "dev": "npm run demo-watch & http-server demo",
    "build-unminified": "webpack --config ./webpack-dist.config.js",
    "build-minified": "MINIFY=true webpack --config ./webpack-dist.config.js",
    "build": "npm run build-unminified && npm run build-minified",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "0.14.x || ^15.0.0",
    "react-dom": "0.14.x || ^15.0.0",
    "react-router": ">=v2.4.0 && ^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "enzyme": "^2.6.0",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "http-server": "^0.9.0",
    "jest": "^18.0.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-test-renderer": "^15.4.1",
    "webpack": "^1.13.2"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  }
}
